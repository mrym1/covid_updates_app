  <main class="d-flex align-items-center justify-content-center">
	<div class="container">
		<div class="row">	
      <div class="col-sm-12 ">
        <div class="col-sm-10 col-sm-offset-1 text-center">
          <div class="four_zero_four_bg">
            <img src="images/404.png" class="img img-fluid rounded-pill" alt="..." style="height: 300px; background-position: center;">
          </div>
		      <div class="contant_box_404">
              <h3 class="h2"> PAGE NOT FOUND. ERROR 404</h3>
              <button class="btn btn-outline-dark" onclick="goBack()">Go Back</button>
	        </div>
		    </div>
		  </div>
		</div>
	</div>
</main>

